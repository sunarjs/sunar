{
	"name": "sunar",
	"version": "0.6.0",
	"description": "A simple and easy to use discord.js framework.",
	"author": "tai (https://github.com/taii03)",
	"license": "MIT",
	"main": "dist/index.cjs",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"type": "module",
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"lint": "biome lint ./src",
		"format": "biome format --write .",
		"test": "vitest run",
		"test:dev": "vitest"
	},
	"keywords": [
		"discord",
		"api",
		"bot",
		"client",
		"node",
		"discord.js",
		"framework",
		"handler",
		"typescript"
	],
	"dependencies": {
		"glob": "11.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.2",
		"@commitlint/cli": "19.5.0",
		"@commitlint/config-conventional": "19.5.0",
		"@total-typescript/tsconfig": "^1.0.4",
		"@types/node": "^22.7.4",
		"dotenv": "^16.4.5",
		"esbuild-plugin-file-path-extensions": "2.1.3",
		"esbuild-plugin-version-injector": "1.2.1",
		"tsup": "8.3.0",
		"tsx": "^4.19.1",
		"typescript": "5.6.2",
		"vitest": "^2.1.1"
	},
	"peerDependencies": {
		"discord.js": "^14.16.x"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./handlers": {
			"types": "./dist/handlers/index.d.ts",
			"import": "./dist/handlers/index.js",
			"require": "./dist/handlers/index.cjs"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"import": "./dist/utils/index.js",
			"require": "./dist/utils/index.cjs"
		},
		"./stores": {
			"types": "./dist/stores/index.d.ts",
			"import": "./dist/stores/index.js",
			"require": "./dist/stores/index.cjs"
		},
		"./registry": {
			"types": "./dist/registry/index.d.ts",
			"import": "./dist/registry/index.js",
			"require": "./dist/registry/index.cjs"
		},
		"./symbols": {
			"types": "./dist/symbols.d.ts",
			"import": "./dist/symbols.js",
			"require": "./dist/symbols.cjs"
		}
	},
	"typesVersions": {
		"*": {
			"handlers": ["./dist/handlers"],
			"utils": ["./dist/utils"],
			"stores": ["./dist/stores"],
			"registry": ["./dist/registry"],
			"symbols": ["./dist/symbols.d.ts"]
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sunarjs/sunar.git"
	},
	"bugs": {
		"url": "https://github.com/sunarjs/sunar/issues"
	},
	"homepage": "https://sunar.js.org",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"engines": {
		"node": ">=18"
	}
}
