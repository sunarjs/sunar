{
    "name": "sunar",
    "description": "A simple and easy to use discord.js framework.",
    "version": "0.7.0",
    "type": "module",
    "files": [
        "dist"
    ],
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        },
        "./client": {
            "import": {
                "types": "./dist/client.d.ts",
                "default": "./dist/client.js"
            }
        },
        "./handlers": {
            "import": {
                "types": "./dist/handlers/index.d.ts",
                "default": "./dist/handlers/index.js"
            }
        },
        "./registry": {
            "import": {
                "types": "./dist/registry/index.d.ts",
                "default": "./dist/registry/index.js"
            }
        },
        "./builders": {
            "import": {
                "types": "./dist/builders/index.d.ts",
                "default": "./dist/builders/index.js"
            }
        },
        "./symbols": {
            "import": {
                "types": "./dist/symbols.d.ts",
                "default": "./dist/symbols.js"
            }
        },
        "./stores": {
            "import": {
                "types": "./dist/stores/index.d.ts",
                "default": "./dist/stores/index.js"
            }
        },
        "./package.json": "./package.json"
    },
    "author": "Usse (https://github.com/ussego)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sunarjs/sunar.git"
    },
    "devDependencies": {
        "bunup": "^0.15.14",
        "typescript": "^5.9.3",
        "vitest": "^4.0.8"
    },
    "peerDependencies": {
        "discord.js": ">=14.x"
    },
    "bugs": {
        "url": "https://github.com/sunarjs/sunar/issues"
    },
    "homepage": "https://sunar.js.org",
    "keywords": [
        "discord",
        "api",
        "bot",
        "client",
        "node",
        "discord.js",
        "framework",
        "handler",
        "typescript"
    ],
    "license": "MIT",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "build": "bunup",
        "test": "vitest run",
        "test:dev": "vitest"
    }
}
