{
	"name": "sunar",
	"version": "0.1.3",
	"description": "A simple and easy to use discord.js framework.",
	"author": "tai (https://github.com/taii03)",
	"license": "MIT",
	"main": "dist/index.cjs",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"type": "module",
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"lint": "biome lint ./src",
		"format": "biome format --write .",
		"prepare": "husky"
	},
	"keywords": [
		"discord",
		"api",
		"bot",
		"client",
		"node",
		"discord.js",
		"framework",
		"handler",
		"typescript"
	],
	"dependencies": {
		"discord.js": "14.15.3",
		"glob": "10.4.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.3",
		"@commitlint/cli": "19.3.0",
		"@commitlint/config-conventional": "19.2.2",
		"esbuild-plugin-file-path-extensions": "2.1.1",
		"esbuild-plugin-version-injector": "1.2.1",
		"husky": "9.0.11",
		"tsup": "8.0.2",
		"typescript": "5.4.5"
	},
	"peerDependencies": {
		"discord.js": "^14.14.x"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./handlers": {
			"types": "./dist/handlers/index.d.ts",
			"import": "./dist/handlers/index.js",
			"require": "./dist/handlers/index.cjs"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"import": "./dist/utils/index.js",
			"require": "./dist/utils/index.cjs"
		},
		"./stores": {
			"types": "./dist/stores/index.d.ts",
			"import": "./dist/stores/index.js",
			"require": "./dist/stores/index.cjs"
		},
		"./registry": {
			"types": "./dist/registry/index.d.ts",
			"import": "./dist/registry/index.js",
			"require": "./dist/registry/index.cjs"
		},
		"./symbols": {
			"types": "./dist/symbols.d.ts",
			"import": "./dist/symbols.js",
			"require": "./dist/symbols.cjs"
		}
	},
	"typesVersions": {
		"*": {
			"handlers": ["./dist/handlers"],
			"utils": ["./dist/utils"],
			"stores": ["./dist/stores"],
			"registry": ["./dist/registry"],
			"symbols": ["./dist/symbols.d.ts"]
		}
	},
	"contributors": ["vamuse <vamusedam@gmail.com>"],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sunarjs/sunar.git"
	},
	"bugs": {
		"url": "https://github.com/sunarjs/sunar/issues"
	},
	"homepage": "https://github.com/sunarjs/sunar",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"engines": {
		"node": ">=16.11.0"
	},
	"packageManager": "bun@1.1.10"
}
